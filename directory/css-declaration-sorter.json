{
	"name": "css-declaration-sorter",
	"version": "7.0.2",
	"description": "Sorts CSS declarations fast and automatically in a certain order.",
	"type": "module",
	"main": "./dist/main.cjs",
	"exports": {
		"import": "./src/main.mjs",
		"require": "./dist/main.cjs"
	},
	"types": "./src/main.d.ts",
	"scripts": {
		"build": "rollup -c",
		"preversion": "npm test",
		"test": "uvu src .+\\.test\\.mjs",
