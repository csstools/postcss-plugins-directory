		"eslint": "^6.6.0",
		"pre-commit": "^1.2.2",
		"rollup": "^1.26.3",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-terser": "^5.1.2"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es6": true,
			"node": true
		},
		"extends": "eslint:recommended",
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": 2018,
			"impliedStrict": true,
			"sourceType": "module"
		},
		"root": true,
		"rules": {
			"no-console": [
				0,
				{
					"allow": [
						"warn",
						"error"
					]
				}
			],
			"require-atomic-updates": [
				0
			]
		}
	},
	"keywords": [
		"postcss",
		"css",
		"postcss-plugin",
		"boilerplate"
	],
