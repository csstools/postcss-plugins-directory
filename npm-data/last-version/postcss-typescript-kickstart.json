{
	"name": "postcss-typescript-kickstart",
	"version": "1.0.1",
	"description": "Simple boilerplate to kick start your new PostCSS plugin with TypeScript",
	"main": "dist/index.js",
	"scripts": {
		"clean": "rimraf dist coverage *.log",
		"compile": "npm run clean && tsc",
		"compile:watch": "tsc --watch",
		"lint": "npm run tslint \"src/**/*.ts\"",
		"test": "nyc ava",
		"test:watch": "ava --watch",
		"tslint": "tslint --project tsconfig.json",
		"precommit": "npm test",
		"pretest": "npm run lint && npm run compile",
		"report-coverage": "codecov -f coverage/lcov.info"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/ezavile/postcss-typescript-kickstart.git"
	},
	"author": {
		"name": "ezavile",
		"email": "ezavile@gmail.com"
	},
	"license": "MIT",
	"keywords": [
		"postcss",
		"postcss-plugin",
		"typescript",
		"boilerplate",
		"kickstart"
	],
	"bugs": {
		"url": "https://github.com/ezavile/postcss-typescript-kickstart/issues"
	},
	"homepage": "https://github.com/ezavile/postcss-typescript-kickstart",
	"ava": {
		"files": [
			"dist/spec/*.spec.js"
		],
		"source": [
			"dist/**/*.js"
		]
	},
	"nyc": {
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"dist/**/*.js"
		],
		"exclude": [
			"dist/**/*.spec.js"
		],
		"reporter": [
			"lcov",
			"text"
		],
		"cache": true,
		"all": true,
		"check-coverage": true
	},
	"devDependencies": {
		"@types/core-js": "0.9.41",
		"@types/node": "7.0.22",
		"ava": "0.19.1",
		"codecov": "2.2.0",
		"core-js": "2.4.1",
		"husky": "0.13.3",
		"nyc": "10.3.2",
		"rimraf": "2.6.1",
		"tslint": "5.1.0",
		"typescript": "2.3.3"
	},
	"dependencies": {
		"postcss": "6.0.1"
	},
