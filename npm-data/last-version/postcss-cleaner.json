{
	"name": "postcss-cleaner",
	"version": "1.2.3",
	"description": "PostCSS plugin to clean unused CSS",
	"keywords": [
		"postcss",
		"css",
		"postcss-plugin",
		"clean",
		"cleaner"
	],
	"author": {
		"name": "Thomas",
		"email": "tbekaert@siliconsalad.com"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tbekaert/postcss-cleaner.git"
	},
	"bugs": {
		"url": "https://github.com/tbekaert/postcss-cleaner/issues"
	},
	"homepage": "https://github.com/tbekaert/postcss-cleaner",
	"dependencies": {
		"postcss": "^6.0.15"
	},
	"devDependencies": {
		"coveralls": "^3.0.0",
		"eslint": "^4.14.0",
		"eslint-config-logux": "^23.0.0",
		"eslint-config-postcss": "^3.0.1",
		"eslint-config-standard": "^11.0.0",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jest": "^21.15.1",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.7.0",
		"eslint-plugin-security": "^1.4.0",
		"eslint-plugin-standard": "^3.1.0",
		"jest": "^22.0.4"
	},
	"scripts": {
		"test": "jest --coverage && eslint *.js",
		"cover": "jest --coverage && open ./coverage/lcov-report/index.html",
		"coveralls": "(cat ./coverage/lcov.info | coveralls) || exit 0",
		"rebuild": "rm -rf node_modules && npm install",
		"precommit": "npm run test"
	},
	"jest": {
		"testEnvironment": "node",
		"collectCoverageFrom": [
			"index.js"
		]
	},
	"eslintConfig": {
		"extends": "eslint-config-postcss",
		"env": {
			"jest": true
		},
		"rules": {
			"no-extra-parens": 0,
			"space-before-function-paren": 0,
			"operator-linebreak": 0,
			"max-len": 0,
			"prefer-rest-params": 0,
			"node/no-extraneous-require": [
				"error",
				{
					"allowModules": [
						"glob"
					]
				}
			],
			"security/detect-non-literal-regexp": 0
		}
	},
