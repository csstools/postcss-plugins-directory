{
	"name": "postcss-color-hsl",
	"version": "2.0.0",
	"description": "PostCSS plugin to transform W3C CSS Color Module Level 4 hsl() new syntax to more compatible CSS (comma-separated hsl() or hsla())",
	"keywords": [
		"postcss",
		"css",
		"postcss-plugin",
		"color",
		"hsl",
		"hsla"
	],
