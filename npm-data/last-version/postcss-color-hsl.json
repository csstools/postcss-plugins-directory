	},
	"bugs": {
		"url": "https://github.com/dmarchena/postcss-color-hsl/issues"
	},
	"homepage": "https://github.com/dmarchena/postcss-color-hsl",
	"main": "dist/index.js",
	"files": [
		"dist",
		"!**/__tests__"
	],
	"dependencies": {
