{
	"name": "usedcss",
	"version": "1.0.7",
	"description": "Extract only styles presented in your html files.",
	"main": "lib/index.js",
	"scripts": {
		"prepublish": "babel src -d lib",
		"test": "eslint src/*js tests/*.js && ./node_modules/.bin/ava && ./node_modules/.bin/jscs src/*.js tests/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/komachi/usedcss.git"
	},
	"keywords": [
		"css",
		"postcss",
		"less",
		"scss",
		"sass",
		"postcss-plugin",
		"optimization"
	],
	"author": {
		"name": "Anton Nesterov"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/komachi/usedcss/issues"
	},
	"homepage": "https://nesterov.pw/usedcss",
	"eslintConfig": {
		"extends": "defaults",
		"env": {
			"node": true,
			"es6": true,
			"mocha": true
		},
		"parserOptions": {
			"sourceType": "module"
		},
		"rules": {
			"quotes": [
				2,
				"single",
				"avoid-escape"
			]
		},
		"globals": {
			"angular": true
		}
	},
	"jscsConfig": {
		"preset": "node-style-guide",
		"requireTrailingComma": null,
		"requireCapitalizedComments": null
	},
	"ava": {
		"require": [
			"babel-register"
		],
		"failFast": true,
		"verbose": true,
		"babel": "inherit",
		"files": [
			"tests/*.tests.js"
		]
	},
	"dependencies": {
		"angular-expressions": "^0.3.0",
		"angular-templatecache-extract": "^1.0.2",
		"bluebird": "^3.3.5",
		"cheerio": "^0.20.0",
		"is-regex": "^1.0.3",
		"multi-glob": "^1.0.1",
		"node-noop": "^1.0.0",
		"postcss": "^5.0.21"
	},
	"devDependencies": {
		"ava": "^0.14.0",
		"babel-cli": "^6.8.0",
		"babel-preset-es2015": "^6.6.0",
		"babel-register": "^6.8.0",
		"eslint": "^2.9.0",
		"eslint-config-defaults": "^9.0.0",
		"jscs": "^3.0.3"
	},
