{
	"name": "postcss-stats",
	"version": "1.1.0",
	"description": "A PostCSS plugin to display stats",
	"keywords": [
		"css",
		"cssstats",
		"postcss",
		"postcss-plugin",
		"stats"
	],
	"homepage": "https://github.com/karolis-sh/postcss-stats#readme",
	"bugs": {
		"url": "https://github.com/karolis-sh/postcss-stats/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/karolis-sh/postcss-stats.git"
	},
	"license": "MIT",
	"author": {
		"name": "Karolis Å arapnickis"
	},
	"main": "lib/index.js",
	"scripts": {
		"build": "babel src --out-dir lib --source-maps",
		"clean": "rimraf lib",
		"develop": "npm run build -- --watch",
		"format": "prettier --write '**/*.*' && sort-package-json",
		"lint": "run-p lint:*",
		"lint:js": "eslint .",
		"lint:md": "markdownlint *.md -i node_modules",
		"qa": "run-p lint test",
		"semantic-release": "semantic-release",
		"test": "jest",
		"travis-deploy-once": "travis-deploy-once",
		"upgrade-interactive": "npm-check --update"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run qa"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.{md,json}": [
			"pretty-quick --staged"
		],
		"package.json": [
			"sort-package-json",
			"git add"
		]
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"jest": {
		"testRegex": "src/.+\\.test\\.js$"
	},
	"dependencies": {
		"brotli-size": "0.1.0",
		"chalk": "^2.4.1",
		"cli-table": "^0.3.1",
		"cssstats": "^3.4.0",
		"gzip-size": "^5.1.1",
		"postcss": "^7.0.18",
		"pretty-bytes": "^5.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.0",
		"@babel/core": "^7.6.0",
		"@babel/preset-env": "^7.6.0",
		"@k.sh/eslint-config": "^1.13.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^24.9.0",
		"babel-plugin-add-module-exports": "^1.0.2",
		"commitizen": "^4.0.3",
		"cssnano": "^4.1.10",
		"cz-conventional-changelog": "^3.0.2",
		"fs-extra": "^8.1.0",
		"husky": "^3.0.5",
		"jest": "^24.9.0",
		"lint-staged": "^9.2.5",
		"markdownlint-cli": "^0.18.0",
		"npm-check": "^5.9.0",
		"npm-run-all": "^4.1.5",
		"postcss-replace": "^1.0.7",
		"pretty-quick": "^1.11.1",
		"rimraf": "^3.0.0",
		"semantic-release": "^15.13.24",
		"sort-package-json": "^1.22.1",
		"travis-deploy-once": "^5.0.11"
	},
